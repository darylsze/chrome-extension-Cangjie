{
  "manifest_version": 2,
  "name": "倉頡字典 － 右 Click 即時查字",
  "description": "右 Click 即時查字",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrwqd12313'; object-src 'self'",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {"page": "index.html"}
}
